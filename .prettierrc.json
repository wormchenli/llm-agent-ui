{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^(cplibrary|cpicons|cputil|cpclient|@cpclient|cpshots).*$",
    "components/.*$",
    "^(constants|data|hooks|util|utils)/.*$",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
